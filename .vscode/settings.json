{
    "[erb]": {
        "editor.defaultFormatter": "aliariff.vscode-erb-beautify",
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "files.trimTrailingWhitespace": true,
        "files.insertFinalNewline": true,
        "files.trimFinalNewlines": true,
        "editor.rulers": [
            150
        ],
        "editor.semanticHighlighting.enabled": true
    },
    "[ruby]": {
        "editor.defaultFormatter": "rubocop.vscode-rubocop",
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "files.trimTrailingWhitespace": true,
        "files.insertFinalNewline": true,
        "files.trimFinalNewlines": true,
        "editor.rulers": [
            120
        ],
        "editor.semanticHighlighting.enabled": true
    },
    "i18n-ally.localesPaths": [
        "config/locales/**",
        "config/locales/**.yml"
    ],
    "i18n-ally.keystyle": "nested",
    "i18n-ally.sortKeys": true,
    "i18n-ally.keepFulfilled": true,
    "i18n-ally.namespace": true,
    "rubyLsp.erbSupport": true,
    "rubyLsp.formatter": "auto",
    "rubyLsp.rubyVersionManager": {
        "identifier": "asdf"
    },
    "rubyLsp.linters": [
        "rubocop",
        "rubocop-factory_bot",
        "rubocop-rails",
        "rubocop-rails_config",
        "rubocop-rake",
        "rubocop-rspec",
        "rubocop-rspec_rails"
    ],
    "rubyLsp.enabledFeatures": {
        "codeActions": true,
        "diagnostics": true,
        "documentHighlights": true,
        "documentLink": true,
        "documentSymbols": true,
        "foldingRanges": true,
        "formatting": true,
        "hover": true,
        "inlayHint": true,
        "onTypeFormatting": true,
        "selectionRanges": true,
        "semanticHighlighting": true,
        "completion": true,
        "codeLens": true,
        "definition": true,
        "workspaceSymbol": true,
        "signatureHelp": true,
        "typeHierarchy": true
    },
    "rubyLsp.customRubyCommand": "asdf",
    "rdbg.rubyVersionManager": "asdf",
    "rdbg.maxTraceLogSize": 50000,
    "rdbg.enableTraceInspector": true,
    "rdbg.enableTraceParameters": true,
    "rdbg.enableTraceReturnValue": true,
    "rdbg.useBundler": true,
    "rdbg.saveBreakpoints": false,
    "solargraph.useBundler": true,
    "solargraph.diagnostics": true,
    "solargraph.autoformat": true,
    "solargraph.completion": true,
    "solargraph.definitions": true,
    "solargraph.references": true,
    "solargraph.hover": true,
    "solargraph.symbols": true,
    "solargraph.folding": true,
    "solargraph.formatting": true,
    "solargraph.rename": true,
    "solargraph.logLevel": "info",
    "solargraph.transport": "socket",
    "solargraph.checkGemVersion": false,
    "diffEditor.codeLens": true,
    "diffEditor.experimental.useTrueInlineView": true,
    "rubocop.safeAutocorrect": true,
    "rubocop.autocorrect": true,
    "rubocop.yjitEnabled": true,
    "rubocop.lintMode": true,
    "rubocop.layoutMode": true,
    "rubocop.mode": "enableUnconditionally",
    "sorbet.lspConfigs": [
        {
            "id": "stable",
            "name": "Sorbet",
            "description": "Stable Sorbet Ruby IDE features",
            "command": [
                "bundle",
                "exec",
                "srb",
                "typecheck",
                "--lsp"
            ]
        },
        {
            "id": "beta",
            "name": "Sorbet (Beta)",
            "description": "Beta Sorbet Ruby IDE features",
            "command": [
                "bundle",
                "exec",
                "srb",
                "typecheck",
                "--lsp",
                "--enable-all-beta-lsp-features"
            ]
        },
        {
            "id": "experimental",
            "name": "Sorbet (Experimental)",
            "description": "Experimental Sorbet Ruby IDE features (warning: crashy, for developers only)",
            "command": [
                "bundle",
                "exec",
                "srb",
                "typecheck",
                "--lsp",
                "--enable-all-experimental-lsp-features"
            ]
        }
    ],
    "taskExplorer.exclude": [
        "**/.vscode-test/**",
        "**/Assets/**",
        "**/bin/**",
        "**/build/**",
        "**/dist/**",
        "**/doc/**",
        "**/example/**",
        "**/examples/**",
        "**/ext/**",
        "**/obj/**",
        "**/out/**",
        "**/output/**",
        "**/packages/**",
        "**/release/**",
        "**/releases/**",
        "**/samples/**",
        "**/sdks/**",
        "**/static/**",
        "**/target/**",
        "**/test/**",
        "**/third_party/**",
        "**/vendor/**",
        "**/work/**",
        "/app/**"
    ],
    "testExplorer.useNativeTesting": false,
    "ruby-spec-runner.minitestCodeLensPrompts": false,
    "ruby-spec-runner.projectPath": "/app",
    "ruby-spec-runner.rspecCommand": "bundle exec rspec",
    "ruby-spec-runner.rspecRunAllFailedButton": true,
    "ruby-spec-runner.rubyDebugger": "Ruby LSP",
    "ruby-spec-runner.windowsTerminalType": "Bash",
    "rubyTestExplorer.logpanel": true,
    "rubyTestExplorer.debugCommand": "rdebug-ide",
    "rubyTestExplorer.testFramework": "rspec",
    "mjml.mailer": "mailcatcher",
    "mjml.autoPreview": true,
}